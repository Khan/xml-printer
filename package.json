{
  "name": "xml-printer",
  "version": "1.1.3",
  "description": "Converts XML AST structures (like the ones generated by xml-reader) to XML strings",
  "main": "dist/index.js",
  "author": "Pedro Ladaria <pedro.ladaria@gmail.com>",
  "scripts": {
    "lint": "eslint ./src",
    "test": "nyc --reporter=lcov ava --verbose test/test.js",
    "report": "nyc report --reporter=html && opn coverage/index.html",
    "clean": "rimraf dist coverage .nyc_output",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run lint && npm test && npm run build",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls"
  },
  "keywords": [
    "formatter",
    "indent",
    "output",
    "printer",
    "render",
    "renderer",
    "serialize",
    "serializer",
    "string",
    "stringify",
    "tostring",
    "writer",
    "xml",
    "document",
    "escape",
    "escaping",
    "text",
    "attribute"
  ],
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.14.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "coveralls": "^2.11.14",
    "eslint": "^3.6.0",
    "nyc": "^8.3.0",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.5.4",
    "xml-reader": "^2.4.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pladaria/xml-printer.git"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pladaria/xml-printer/issues"
  },
  "homepage": "https://github.com/pladaria/xml-printer#readme",
  "directories": {
    "test": "test"
  }
}
